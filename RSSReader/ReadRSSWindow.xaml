<Window x:Class="RSSReader.ReadRSSWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RSSReader"
        mc:Ignorable="d"
        Title="ReadRSSWindow" Height="300" Width="300">
    <Grid>
        <Button x:Name="btn_Close" Content="Zamknij" Margin="207,239,0,0" VerticalAlignment="Center" Width="75" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Center"/>
        <RichTextBox x:Name="RTB_RSS" HorizontalAlignment="Left" Height="224" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" HorizontalContentAlignment="Center">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
